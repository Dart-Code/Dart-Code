{
	"name": "Dart-Code Dev Container",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"workspaceMount": "source=${localWorkspaceFolder},target=/workspaces/Dart-Code,type=bind",
	"workspaceFolder": "/workspaces/Dart-Code",
	"customizations": {
		"vscode": {
			"extensions": [
				"amodio.tsl-problem-matcher",
				"dbaeumer.vscode-eslint"
			]
		}
	},
	"postCreateCommand": "npm install"
}
