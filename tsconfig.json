{
	// Try to keep synced with Flutter extension!
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "out/src",
		"lib": [
			"es6"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true
	},
	"exclude": [
		".vscode-test",
		"node_modules",
		"out",
		"src/test/test_projects"
		// tsconfig.build.json excludes additional paths
	]
}
