{
	// Try to keep synced with Flutter extension!
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "out/src",
		"lib": [
			"es6"
		],
		"sourceMap": true,
		"rootDir": "src",
		"noImplicitAny": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"strictFunctionTypes": true,
		//"strictPropertyInitialization": true,
		//"strictNullChecks": true,
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}
